{
  "name": "@weareopensource/vue",
  "description": "Node - Boilerplate Back : Express, Jwt, Mongo, Sequelize (Beta)  (Based on meanjs and riess.js)",
  "version": "0.1.0",
  "keywords": [
    "vue",
    "vuex",
    "vuetify",
    "jwt",
    "jest",
    "stack",
    "boilerplate"
  ],
  "private": false,
  "author": "https://github.com/weareopensource/Vue/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weareopensource/Vue.git"
  },
  "type": "module",
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.2"
  },
  "scripts": {
    "start": "run-s generateConfig vue:serve",
    "build": "run-s generateConfig vue:build",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "lint": "./node_modules/.bin/eslint --fix ./src",
    "lint:vue": "vue-cli-service lint",
    "test": "",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "test:lint": "./node_modules/.bin/eslint ./src",
    "snyk-protect": "snyk protect",
    "commit": "npx cz",
    "release": "standard-version",
    "release:auto": "npx semantic-release",
    "generateConfig": "node ./scripts/generateConfig.js"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@tryghost/content-api": "1.7.1",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.3",
    "axios": "^0.26.1",
    "babel-runtime": "^6.26.0",
    "core-js": "^3.21.1",
    "lodash": "^4.17.21",
    "marked": "^4.0.12",
    "moment": "^2.29.1",
    "snyk": "^1.872.0",
    "vue": "=2.6.14",
    "vue-gravatar": "^1.4.1",
    "vue-loader": "^17.0.0",
    "vue-matomo": "^4.1.0",
    "vue-meta": "^2.4.0",
    "vue-router": "=3.5.3",
    "vuetify": "^2.6.4",
    "vuex": "=3.6.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@vue/cli-plugin-babel": "^5.0.1",
    "@vue/cli-plugin-e2e-cypress": "^5.0.3",
    "@vue/cli-plugin-eslint": "^5.0.1",
    "@vue/cli-plugin-router": "^5.0.1",
    "@vue/cli-plugin-unit-jest": "^5.0.1",
    "@vue/cli-plugin-vuex": "^5.0.3",
    "@vue/cli-service": "^5.0.1",
    "@vue/eslint-config-airbnb": "^6.0.0",
    "@vue/test-utils": "1.3.0",
    "@weareopensource/conventional-changelog": "^1.7.0",
    "chalk": "^5.0.0",
    "commitizen": "^4.2.4",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-markdown": "^2.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.8",
    "prettier": "^2.6.0",
    "sass": "<1.50",
    "sass-loader": "12.6.0",
    "semantic-release": "^19.0.2",
    "standard-version": "^9.3.2",
    "vue-cli-plugin-vuetify": "^2.4.7",
    "vue-template-compiler": "^2.6.14",
    "vuetify-loader": "^1.7.3",
    "webpack": "^5.69.1"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      }
    ],
    "ci": false,
    "repositoryUrl": "https://github.com/weareopensource/Vue.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failComment": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@weareopensource/conventional-changelog"
    }
  },
  "snyk": true
}
