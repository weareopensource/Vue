{
  "name": "@weareopensource/vue",
  "description": "Node - Boilerplate Back : Express, Jwt, Mongo, Sequelize (Beta)  (Based on meanjs and riess.js)",
  "version": "0.1.0",
  "keywords": [
    "vue",
    "vuex",
    "vuetify",
    "jwt",
    "jest",
    "stack",
    "boilerplate"
  ],
  "private": false,
  "author": "https://github.com/weareopensource/Vue/graphs/contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/weareopensource/Vue.git"
  },
  "engines": {
    "node": ">=10.16.3",
    "npm": ">=6.11.3"
  },
  "scripts": {
    "start": "run-s generateConfig vue:serve",
    "build": "run-s generateConfig vue:build",
    "vue:serve": "vue-cli-service serve",
    "vue:build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "commit": "npx git-cz",
    "release": "standard-version",
    "release:auto": "npx semantic-release",
    "generateConfig": "node ./scripts/generateConfig.js"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@tryghost/content-api": "^1.4.2",
    "aos": "^2.3.4",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "lodash": "^4.17.19",
    "snyk": "^1.369.2",
    "vue": "^2.6.11",
    "vue-markdown": "^2.2.4",
    "vue-matomo": "^3.13.5-0",
    "vue-router": "^3.3.4",
    "vuetify": "^2.3.7",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@vue/cli-plugin-babel": "^4.4.6",
    "@vue/cli-plugin-e2e-cypress": "^4.4.6",
    "@vue/cli-plugin-eslint": "^4.4.6",
    "@vue/cli-plugin-router": "^4.5.2",
    "@vue/cli-plugin-unit-jest": "^4.4.6",
    "@vue/cli-plugin-vuex": "^4.4.6",
    "@vue/cli-service": "^4.4.6",
    "@vue/eslint-config-airbnb": "^5.1.0",
    "@vue/test-utils": "1.0.3",
    "@weareopensource/conventional-changelog": "^1.7.0",
    "babel-eslint": "^10.1.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.1.2",
    "eslint": "^7.6.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "object-path": "^0.11.4",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.2",
    "semantic-release": "^17.1.1",
    "standard-version": "^8.0.2",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.6.0"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      }
    ],
    "ci": false,
    "repositoryUrl": "https://github.com/weareopensource/Vue.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failComment": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "message": "chore(release): ${nextRelease.version} \n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@weareopensource/conventional-changelog"
    }
  },
  "snyk": true
}
